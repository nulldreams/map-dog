<!doctype html>
<html class="no-js" ng-app="MapApp">
<head>
    <meta charset="utf-8">
    <title>Scotch 
    Map</title>
    <meta name="description">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- CSS -->
    <link rel="stylesheet" href="../bower_components/bootstrap/dist/css/bootstrap.css"/>
    <link rel="stylesheet" href="style.css"/>

    <!-- Google Maps API -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVGi9XPz_CX3WHEsRjey1E4DuQ78d8t4A"></script>

    <!-- JS Source -->
    <script src="../bower_components/jquery/dist/jquery.js"></script>
    <script src="../bower_components/angular/angular.js"></script>
    <script src="../bower_components/angularjs-geolocation/dist/angularjs-geolocation.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n" crossorigin="anonymous"></script>
    <!-- Angular Files -->
    <script src="js/app.js"></script>
    <script src="js/gservice.js"></script>
    <script src="js/headerCtrl.js"></script>
</head>
<body>
<div class="container" ng-controller="queryCtrl">
    <!-- Header Menu. Note use of headerCtrl -->
    <div class="header" ng-controller="headerCtrl">
        <h3 class="text-muted">Mapa</h3>
         <h1>Ol√° <%= user.local.name || user.facebook.name %>, vamos ajudar um dog zica hoje?</h1>
    </div>
    <div class="row content" ng-controller="addCtrl">

        <!-- Google Map -->
        <div class="col-md-12">
            <div id="map"></div>
        </div>
        <div ng-view></div>

    </div>
    <hr/>
    <!-- Footer -->
    <div class="footer">
        <p class="text-center"><span class="glyphicon glyphicon-check"></span> Bichinhos
         <a href="/logout" class="btn btn-default btn-sm">Logout</a></p>
    </div>

</div>
</body>
